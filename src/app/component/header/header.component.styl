.header
  position relative
  background-image url(../../../assets/images/top-bg.jpg)
  height 60px
  vertical-align middle
  z-index 10
  .logo
    position absolute
    left 40px
    top: 50%
    margin-top -15px

  .operator
    float right
    ul
      li
        height 60px
        line-height 60px
        float left
        padding 0 34px
        font-size 16px
        color #fff
        cursor pointer
        &.active,&:hover
          background #1179f6